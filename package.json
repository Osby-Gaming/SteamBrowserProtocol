{
  "name": "steambrowserprotocol",
  "module": "src/SteamBrowserProtocol.ts",
  "type": "module",
  "version": "0.1.1",
  "exports": {
    "types": "./dist/ts/SteamBrowserProtocol.d.ts",
    "import": "./dist/esm/SteamBrowserProtocol.js",
    "require": "./dist/cjs/SteamBrowserProtocol.cjs"
  },
  "scripts": {
    "build": "concurrently \"bun run build-browser\" \"bun run build-node\" \"bun run build-bun\" \"bun run build:declaration\"",
    "build-browser": "bun build ./src/SteamBrowserProtocol.ts --outdir ./dist/browser --target browser",
    "build-node": "bun build ./src/SteamBrowserProtocol.ts --outdir ./dist/cjs --target node --format cjs",
    "build-bun": "bun build ./src/SteamBrowserProtocol.ts --outdir ./dist/esm --target bun",
    "dev": "concurrently \"bun run dev-browser\" \"bun run dev-node\" \"bun run dev-bun\" \"bun run dev:declaration\"",
    "dev-browser": "bun build ./src/SteamBrowserProtocol.ts --outdir ./dist/browser --target browser --watch",
    "dev-node": "bun build ./src/SteamBrowserProtocol.ts --outdir ./dist/cjs --target node --format cjs --watch",
    "dev-bun": "bun build ./src/SteamBrowserProtocol.ts --outdir ./dist/esm --target bun --watch",
    "build:declaration": "tsc ./src/SteamBrowserProtocol.ts --emitDeclarationOnly --declaration --outFile ./dist/ts/SteamBrowserProtocol.js",
    "dev:declaration": "tsc ./src/SteamBrowserProtocol.ts --emitDeclarationOnly --declaration --outFile ./dist/ts/SteamBrowserProtocol.js --watch",
    "demo": "concurrently \"bun run dev-browser\" \"bun ./demo/server.ts\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "open": "^11.0.0"
  }
}
